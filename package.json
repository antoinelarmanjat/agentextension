{
  "name": "agentdesigner",
  "displayName": "AgentDesigner",
  "description": "An extension to design ADK agents",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:agentdesigner.helloWorld",
    "onCommand:agentdesigner.insertDateTime",
    "onCommand:agentdesigner.scanDirectory"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "agentdesigner.helloWorld",
        "title": "Hello World"
      },
      {
        "command": "agentdesigner.insertDateTime",
        "title": "Insert Date Time"
      },
      {
        "command": "agentdesigner.scanDirectory",
        "title": "Scan Directory for Agents"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.85.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "eslint": "^9.32.0",
    "typescript": "^5.9.2",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}
